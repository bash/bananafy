{
  "manifest_version": 2,
  "name": "Bananafy",
  "homepage_url": "https://github.com/bash/bananafy",
  "version": "0.0.2",
  "description": "🍌 replaces words randomly with banana",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/inject.js"
      ]
    }
  ]
}
